<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
        
    <TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<Button android:layout_height="wrap_content" android:id="@+id/startButton" android:layout_width="wrap_content" android:text="Start"></Button>
		<Button android:layout_height="wrap_content" android:id="@+id/stopButton" android:layout_width="wrap_content" android:text="Stop"></Button>
		<Button android:layout_height="wrap_content" android:id="@+id/quitButton" android:layout_width="wrap_content" android:text="Quit"></Button>
	</TableRow>
	
	<TableRow android:id="@+id/TableRow05" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<Button android:text="Show Buffered Data" android:id="@+id/DataShowButton" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
		<Button android:text="Clear Buffered Data" android:id="@+id/DataClearButton" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
	</TableRow>
	<Button android:text="Send Data To Server" android:id="@+id/SendDataButton" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button><TableRow android:id="@+id/TableRow02" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:text="Building/Context" android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView><Spinner android:id="@+id/building" android:layout_width="wrap_content" android:layout_height="wrap_content"></Spinner>
			
		
	

</TableRow>
	
	<TableRow android:id="@+id/TableRow03" android:layout_width="wrap_content" android:layout_height="wrap_content">
	
	<TextView android:text="Location ID" android:id="@+id/TextView02" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
	<EditText android:text="Location Identifier" android:id="@+id/EditText01" android:layout_width="wrap_content" android:layout_height="wrap_content"></EditText>

</TableRow>

<Button android:text="Set" android:layout_width="wrap_content" android:id="@+id/setButton" android:layout_height="wrap_content"></Button><TextView android:text="building &gt; checkpoint" android:id="@+id/phonedesc" android:layout_width="fill_parent" android:layout_height="wrap_content"></TextView><TableRow android:id="@+id/TableRow04" android:layout_width="wrap_content" android:layout_height="wrap_content">
<TextView  
    android:layout_width="fill_parent" 
    android:layout_height="wrap_content" 
    android:text="Location description"
    android:id="@+id/debugText"
    />
</TableRow>


<LinearLayout android:id="@+id/LinearLayout01" android:layout_width="wrap_content" android:layout_height="wrap_content">
	<ProgressBar style="@android:style/Widget.ProgressBar.Horizontal" android:id="@+id/ProgressBar01" android:layout_height="wrap_content" android:layout_width="wrap_content"></ProgressBar>
</LinearLayout>


<TableRow android:id="@+id/TableRow06" android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:text="Experiment Done?" android:id="@+id/NetAccessProgressDone" 
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
</TableRow>


<TableRow android:id="@+id/TableRow07" android:layout_width="wrap_content" android:layout_height="wrap_content">
	<EditText android:text="NetAccessStream" android:id="@+id/NetAccessStream" android:layout_width="wrap_content" 
		android:layout_height="wrap_content" android:scrollbars="vertical" ></EditText>
</TableRow>








</LinearLayout>
